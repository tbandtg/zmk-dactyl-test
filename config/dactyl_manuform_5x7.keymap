#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/rgb.h>
#include <dt-bindings/zmk/ext_power.h>
#include <dt-bindings/zmk/outputs.h>

#define DEFAULT 0
#define LWR 1
#define RSE 2
#define ADJ 3

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp ESC   &kp N1   &kp N2 &kp N3 &kp N4 &kp N5 &kp N6                     &kp N7   &kp N8 &kp N9 &kp N0    &kp MINUS &kp EQUAL &kp BSPC
                &kp TAB   &kp Q  &kp W    &kp E  &kp R  &kp T  &kp LBKT                   &kp RBKT &kp Y  &kp U  &kp I     &kp O     &kp P     &kp MINUS
                &kp LSHFT &kp A  &kp S    &kp D  &kp F  &kp G  &kp LPAR                   &kp RPAR &kp H  &kp J  &kp K     &kp L     &kp SEMI  &kp SQT
                &kp LCTRL &kp Z  &kp X    &kp C  &kp V  &kp B                                      &kp N  &kp M  &kp COMMA &kp DOT   &kp FSLH  &kp BSLH
                &kp CAPS  &kp LGUI &kp LBKT &kp RBKT                                                             &kp LEFT  &kp DOWN  &kp UP    &kp RIGHT
                                                    &mo LWR  &kp SPACE                           &kp RET  &mo RSE
                                                             &kp TAB  &kp HOME   &kp END  &kp DEL
                                                             &kp BSPC &mo ADJ    &kp LGUI &kp LALT
            >;                                                                    
        };                                                                        
																	              
        lower_layer {                                                             
            bindings = <                                                          
                &trans   &kp F1    &kp F2    &kp F3    &kp F4    &kp F5 &kp F6          &kp F7 &kp F8    &kp F9   &kp F10  &kp F11   &kp F12  &trans
                &trans   &trans    &trans    &trans    &trans    &trans &trans          &trans &kp PG_UP &kp HOME &kp UP   &kp END   &kp DEL  &trans
                &kp CAPS &kp LGUI  &kp LALT  &kp LSHFT &kp LCTRL &trans &trans          &trans &kp PG_DN &kp LEFT &kp DOWN &kp RIGHT &kp BSPC &trans
                &trans   &kp LC(Z) &kp LC(X) &kp LC(C) &kp LC(V) &trans                        &kp INS   &trans   &trans   &trans    &trans   &trans
                &trans   &trans    &trans    &trans                                                               &trans   &trans    &trans   &trans
                                                    &trans &trans                                    &trans &mo ADJ
                                                           &trans &trans                      &trans &trans
                                                           &trans &trans                      &trans &trans											 
            >;                                                                    
        };                                                                        
																	              
        raise_layer {                                                             
            bindings = <                                                          
                &kp GRAVE    &kp EXCL &kp AT    &kp HASH  &kp DLLR  &kp PRCNT &trans         &trans &kp CARET &kp AMPS &kp ASTRK &kp LPAR &kp RPAR &trans
                &kp K_VOL_UP &kp HOME &kp PG_DN &kp PG_UP &kp END   &trans    &trans         &trans &trans    &trans   &trans    &trans   &trans   &kp K_VOL_UP
                &kp K_VOL_DN &kp LEFT &kp DOWN  &kp UP    &kp RIGHT &trans    &trans         &trans &trans    &trans   &trans    &trans   &trans   &kp K_VOL_DN
                &kp K_MUTE   &trans   &trans    &trans    &trans    &trans                          &trans    &trans   &trans    &trans   &trans   &kp K_MUTE
                &trans       &trans   &trans    &trans                                                                 &trans    &trans   &trans   &trans
                                                     &mo ADJ &trans                                    &trans &trans
                                                             &trans &trans                      &trans &trans
                                                             &trans &trans                      &trans &trans											 
            >;
        };

        adjust_layer {
            bindings = <
                &out OUT_BLE &bt BT_SEL 0 &bt BT_SEL 1 &bt BT_SEL 2 &bt BT_SEL 3 &bt BT_SEL 4 &none       &none &rgb_ug RGB_EFF &rgb_ug RGB_HUI &rgb_ug RGB_SAI &rgb_ug RGB_SPI &rgb_ug RGB_BRI &ext_power EP_TOG
                &out OUT_USB &none        &none        &none        &reset       &none        &none       &none &rgb_ug RGB_EFR &rgb_ug RGB_HUD &rgb_ug RGB_SAD &rgb_ug RGB_SPD &rgb_ug RGB_BRD &rgb_ug RGB_TOG
                &none        &none        &none        &none        &none        &none        &none       &none &none &none  &none &none &none &none
                &bt BT_CLR   &none        &none        &none        &none        &bootloader              &bootloader &none  &none &none &none &none
                &none        &none        &none        &none                                                                 &none &none &none &none
                                                            &none &none                                         &none &none
                                                                  &none &none                             &none &none
                                                                  &none &none                             &none &none
            >;
        };
    };
};
